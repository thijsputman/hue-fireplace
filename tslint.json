{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-sonarts",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "member-ordering": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prettier": true,
    "trailing-comma": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-invalid-await": true
  },
  "rulesDirectory": []
}
